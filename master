# print-alternate-Array
//20th program on c 
#include<stdio.h>
int main ()
{
int a[100],n,i;
printf("Enter the size of the array \n");
scanf("%d",&n);
printf("Enter the elelments of the array \n");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("The alternate elements of the array are \n");
for(i=0;i<n;i+=2)
{
printf("%d ",a[i]);
}
return(0);
}

